@model IEnumerable<lab03.Models.Book>
@{
}
@foreach (var item in Model)
{
    <div class="col-xs-3 col-sm-3 col-md-3 col-lg-3 ">
        <div class="thumbnail">
            <img src="@item.Image" alt="">
            <div class="caption">
                <h3>@item.Title</h3>
                <p>
                    Price: @item.Price
                </p>
            </div>
        </div>
    </div>
}
